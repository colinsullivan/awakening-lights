{
  "name": "awakening-lights",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "bootstrap": "^4.4.1",
    "colorjoe": "^4.1.0",
    "onecolor": "^3.1.0",
    "react": "^16.12.0",
    "react-color": "^2.18.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.3.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "uuid": "^3.4.0",
    "ws": "^7.2.1"
  },
  "scripts": {
    "start_react": "react-scripts start",
    "start_fadecandy": "./git_modules/fadecandy/bin/fcserver-osx",
    "start_server_dev": "NODE_ENV=development babel-node src/server/server_main.js",
    "start": "concurrently \"npm run start_react\" \"npm run start_server_dev\"",
    "start_server": "node build/server/server_main.js",
    "build_react": "react-scripts build",
    "build_server": "babel src/server --out-dir build/server && babel src/common --out-dir build/common",
    "build": "npm run build_react && npm run build_server",
    "test": "npm run build && react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": false,
      "es6": true,
      "mocha": false
    },
    "rules": {
      "no-console": 0,
      "no-use-before-define": "error",
      "block-scoped-var": "error",
      "no-case-declarations": 0,
      "prefer-const": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@testing-library/dom": "^6.11.0",
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "node-sass": "^4.13.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "proxy": "http://localhost:8080"
}
